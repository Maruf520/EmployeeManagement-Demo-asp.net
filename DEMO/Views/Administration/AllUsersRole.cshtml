@model DEMO.Models.RoleUserViewModel

@{
    ViewData["Title"] = "AllUsersRole";
}

<h1>AllUsersRole</h1>

<div>
    <h4>RoleUserViewModel</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @foreach (var item in ViewBag.role)
            {
                @item.RoleName
                @item.UserName
            }
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class="col-sm-2">
            @foreach (var item in ViewBag.user )
            {
                
                @item.UserName
                @item.UserRole
            }
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RoleName)
        </dd>
    </dl>
</div>
